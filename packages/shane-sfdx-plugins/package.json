{
  "name": "shane-sfdx-plugins",
  "description": "sfdx plugins by Shane McLaughlin",
  "version": "0.12.0",
  "author": "@mshanemc",
  "bugs": "https://github.com/mshanemc/shane-sfdx-plugins/issues",
  "dependencies": {
    "@oclif/command": "^1.4.7",
    "@oclif/config": "^1.3.62",
    "@oclif/errors": "^1.0.3",
    "@salesforce/command": "0.0.10",
    "chalk": "^2.3.2",
    "fs-extra": "^5.0.0",
    "js2xmlparser": "^3.0.0",
    "npm": "^5.8.0",
    "request": "^2.85.0",
    "request-promise-native": "^1.0.5",
    "xml2js": "^0.4.19"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.13.0",
    "@oclif/plugin-help": "^1.2.2",
    "@oclif/test": "^1.0.1",
    "@oclif/tslint": "^1.0.2",
    "@salesforce/dev-config": "^1.0.4",
    "@types/chai": "^4.1.2",
    "@types/fs-extra": "^5.0.1",
    "@types/mocha": "^5.0.0",
    "@types/node": "^9.6.2",
    "@types/request-promise-native": "^1.0.14",
    "chai": "^4.1.2",
    "globby": "^8.0.1",
    "mocha": "^5.0.5",
    "sinon": "^4.5.0",
    "ts-node": "5",
    "tslint": "^5.9.1",
    "typescript": "^2.8.1"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    ".oclif.manifest.json",
    "/lib",
    "/messages"
  ],
  "homepage": "https://github.com/mshanemc/shane-sfdx-plugins",
  "keywords": [
    "sfdx-plugin"
  ],
  "license": "MIT",
  "oclif": {
    "commands": "./lib/commands",
    "topics": {
      "shane:org": {
        "description": "create an org with a friendly username or delete an org without a username"
      },
      "shane:user": {
        "description": "set a users password or their chatter profile/banner photo"
      },
      "shane:mdapi": {
        "description": "pull unpackaged or packaged source from an org"
      },
      "shane:object": {
        "description": "create objects locally or add fields to them"
      }
    },
    "bin": "sfdx",
    "devPlugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "mshanemc/shane-sfdx-plugins",
  "scripts": {
    "build": "rm -rf lib && tsc",
    "clean": "rm -f .oclif.manifest.json",
    "lint": "tsc -p test --noEmit && tslint -p test -t stylish",
    "postpublish": "yarn run clean",
    "posttest": "yarn run lint",
    "prepare": "yarn run build && oclif-dev manifest",
    "prepublishOnly": "yarn run build && oclif-dev manifest",
    "preversion": "yarn run clean",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  }
}
